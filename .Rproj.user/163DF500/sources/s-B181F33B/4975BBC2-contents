library(qiime2R)
library(Biostrings)
S12_rep_seq <- read_qza("12S_exports/dada2_output/rep_seqs_final.qza")
writeXStringSet(S12_rep_seq$data, "S12_rep_seq", format="fasta")

S16_rep_seq <- read_qza("16S_exports/dada2_output/rep_seqs_final.qza")
writeXStringSet(S16_rep_seq$data, "S16_rep_seq", format="fasta")

S18_rep_seq <- read_qza("18S_exports/dada2_output/rep_seqs_final.qza")
writeXStringSet(S18_rep_seq$data, "S18_rep_seq", format="fasta")
